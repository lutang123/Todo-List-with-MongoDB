<%- include("header") -%>

  <div class="box" id="heading">
    <h1> <%= listTitle %></<h1>
    <P class="completedP">You have completed your ToDo list today!" </P>
  </div>

  <div class="box">
    <% deletedItems.forEach(function(item) { %>
      <div class="item">
        <!-- // this onchange is to make the checkbox in input get submitted to the form. -->
        <p class="deletedItem"><%=item.name%></p>
      </div>
    <% }) %>
  </div>

  <div class="box goDream">
    <!-- <form action="/dream" method="post"> -->
